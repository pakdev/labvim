#:kivy 1.1.0

<Main>:
    Grid:
        id: grid
    Cursor:
        main: root
        thickness: 2
        canvas:
            Color:
                rgba: (1, 1, 0, 0.7)
            Line:
                rectangle: (0, 0, grid.cell_width, grid.cell_height)
                width: self.thickness


